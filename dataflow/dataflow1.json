{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "OutputSqlOrders",
						"type": "DatasetReference"
					},
					"name": "orders"
				},
				{
					"dataset": {
						"referenceName": "OutputMenyItems",
						"type": "DatasetReference"
					},
					"name": "items"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "TableSaleDataBase",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "Join"
				},
				{
					"name": "Sort"
				},
				{
					"name": "filter1"
				},
				{
					"name": "RemoveColumns1",
					"description": "Autogenerated by data preview actions"
				}
			],
			"scriptLines": [
				"source(output(",
				"          order_details_id as integer,",
				"          order_id as integer,",
				"          order_date as string,",
				"          order_time as string,",
				"          item_id as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> orders",
				"source(output(",
				"          menu_item_id as string,",
				"          item_name as string,",
				"          category as string,",
				"          price as decimal(10,2)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> items",
				"Sort, items join(item_id == menu_item_id,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> Join",
				"orders sort(desc(order_id, true)) ~> Sort",
				"Join filter(item_id != '') ~> filter1",
				"filter1 select(mapColumn(",
				"          order_details_id,",
				"          order_id,",
				"          order_date,",
				"          order_time,",
				"          item_id,",
				"          item_name,",
				"          category,",
				"          price",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> RemoveColumns1",
				"RemoveColumns1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          order_details_id as integer,",
				"          order_id as integer,",
				"          order_date as string,",
				"          order_time as string,",
				"          item_id as string,",
				"          item_name as string,",
				"          category as string,",
				"          price as decimal(10,2)",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sink1"
			]
		}
	}
}